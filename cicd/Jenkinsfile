def lib

node('worker') {
  stage('Checkout') {
    checkout scm
  }

  stage('Load') {
    lib = load 'cicd/functions.groovy'
  }

  stage('Execute') {
    steps {
    parallel(
      a: {
        echo "This is branch a"
      },
      b: {
        echo "This is branch b"
      }
    )
  }
  }

	stage('Tag') {
		lib.tagBuild()
	}


}

