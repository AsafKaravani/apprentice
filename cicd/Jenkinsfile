def lib

node('worker') {
  stage('Checkout') {
    checkout scm
  }

  stage('Load') {
    lib = load 'cicd/functions.groovy'
  }

  stage('Execute') {
    lib.tagBuild()
  }

	stage('Tag') {
		lib.tagBuild()
	}

	parallel {
		stage('1') {
     	sh 'echo "1"'
  	}
		stage('2') {
		 	sh 'echo "2"'
		}
		stage('3') {
		 	sh 'echo "3"'
		}
	}
}

